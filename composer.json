{
    "name": "comporu/compo-core",
    "type": "library",
    "description": "compo-core",
    "keywords": [
        "compo",
        "symfony",
        "sonata"
    ],
    "homepage": "https://compo.ru",
    "license": "MIT",
    "authors": [
        {
            "name": "Compo.ru",
            "email": "info@compo.ru",
            "homepage": "https://compo.ru"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-develop": "3.3.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Compo\\": "src/Compo",
            "Pix\\SortableBehaviorBundle\\": "src/Pix/PixSortableBehaviorBundle",
            "Sylius\\Bundle\\SettingsBundle\\": "src/Sylius/SyliusSettingsBundle",
            "Sylius\\Bundle\\ThemeBundle\\": "src/Sylius/SyliusThemeBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Compo\\Tests\\": "tests/"
        }
    },
    "support": {
        "email": "info@compo.ru",
        "issues": "https://github.com/comporu/compo-core/issues",
        "source": "https://github.com/comporu/compo-core",
        "docs": "http://docs.compo-symfony-cms.ru/"
    },
    "require": {
        "php": ">=7.2.0",

        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-openssl": "*",
        "ext-iconv": "*",

        "symfony/symfony": "^3.4",
        "symfony/assetic-bundle": "^2.8",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/monolog-bundle": "^3.1",

        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0 || ^5.1",
        "sensio/generator-bundle": "^3.1",

        "doctrine/doctrine-migrations-bundle": "^1.3",

        "friendsofsymfony/jsrouting-bundle": "^2.0",
        "friendsofsymfony/rest-bundle": "^2.3",
        "friendsofsymfony/elastica-bundle": "^5.0",

        "sonata-project/admin-bundle": "^3.31",
        "sonata-project/block-bundle": "^3.12",
        "sonata-project/cache": "^1.1",
        "sonata-project/cache-bundle": "^2.4",
        "sonata-project/core-bundle": "^3.9",
        "sonata-project/datagrid-bundle": "^2.3",
        "sonata-project/doctrine-extensions": "^1.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.4",
        "sonata-project/easy-extends-bundle": "^2.5",
        "sonata-project/exporter": "^1.8",
        "sonata-project/formatter-bundle": "^3.4",
        "sonata-project/intl-bundle": "^2.4",
        "sonata-project/media-bundle": "^3.10",
        "sonata-project/notification-bundle": "^3.3",
        "sonata-project/page-bundle": "^3.7",
        "sonata-project/seo-bundle": "^2.5",
        "sonata-project/timeline-bundle": "^3.3",
        "sonata-project/user-bundle": "^4.1",
        "sonata-project/dashboard-bundle": "^0.2",

        "sylius/resource-bundle": "^1.0.4",
        "sylius/registry": "^1.0.4",
        "sylius/resource": "^1.0.4",

        "jms/translation-bundle": "^1.3",
        "jms/job-queue-bundle": "^1.4",
        "presta/sitemap-bundle": "^1.5",
        "konstantin-kuklin/assetic-static-gzip-bundle": "^1.0",
        "simplethings/entity-audit-bundle": "^1.0",
        "nelmio/api-doc-bundle": "^2.13",
        "stecman/symfony-console-completion": "^0.7",
        "liip/imagine-bundle": "^1.9",
        "yectep/phpspreadsheet-bundle": "^0.0",
        "knplabs/knp-paginator-bundle": "^2.7",
        "beberlei/doctrineextensions": "^1.0",
        "zendframework/zend-hydrator": "^2.3",
        "craue/formflow-bundle": "^3.0",
        "mopa/bootstrap-bundle": "^3.2",
        "twbs/bootstrap": "^3.3",
        "twbs/bootstrap-sass": "^3.3",
        "debril/rss-atom-bundle": "^4.0",
        "behat/mink-extension": "^2.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/symfony2-extension": "^2.1",
        "behat/behat": "^3.4",
        "liip/monitor-bundle": "^2.6",
        "nixsolutions/yandex-php-library": "^3.0",

        "evence/soft-deleteable-extension-bundle": "^1.3",

        "emuse/behat-html-formatter": "0.1.x-dev"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.7",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "symfony/dotenv": "^4.0"
    },
    "config": {
        "sort-packages": false,
        "bin-dir": "bin"
    }
}
